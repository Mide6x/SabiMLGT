{
  "expo": {
    "name": "SabiML",
    "slug": "SabiML",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "permissions": [
      "network"
    ],
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SabiML"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "681be368-d25e-4e6e-86fa-f0d497238846"
      }
    }
  }
}
